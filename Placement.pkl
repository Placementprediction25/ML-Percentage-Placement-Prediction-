    "## Train - Test Split "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "9e6378a9-3792-4092-a7f9-4feecd2ceb7f",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "def split_train_test(data,test_ratio):\n",
    "    np.random.seed(42)\n",
    "    shuffled = np.random.permutation(len(data))\n",
    "    print(shuffled)\n",
    "    test_set_size = int(len(data)*test_ratio)\n",
    "    test_indices = shuffled[:test_set_size]\n",
    "    train_indices = shuffled[test_set_size:]\n",
    "    return data.iloc[train_indices], data.iloc[test_indices] \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "e98f58a5-e8b0-41bf-a8c6-3d530e458336",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[ 107 5484 6998 ... 5390  860 7270]\n",
      "rows in test set: 2200\n",
      "rows in train set: 8800\n"
     ]
    }
   ],
   "source": [
    "train_set, test_set =  split_train_test(dataset,0.2)\n",
    "print(f\"rows in test set: {len(test_set)}\")\n",
    "print(f\"rows in train set: {len(train_set)}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "4fff9331-31c0-4d27-aabe-58f079094422",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.model_selection import StratifiedShuffleSplit\n",
    "split = StratifiedShuffleSplit(n_splits=1, test_size=0.2, random_state=42)\n",
    "for train_index, test_index in split.split(dataset, dataset['Projects']):\n",
    "    strat_train_set = dataset.loc[train_index]\n",
    "    strat_test_set = dataset.loc[test_index]"
   ]
